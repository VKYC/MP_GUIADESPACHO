<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_picking_form_inherit_office_guide" model="ir.ui.view">
        <field name="name">stock.picking.view.form.inherit</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="get_url_pdf_dte" type="object" string="Get url PDF" attrs="{'invisible': [('state', '!=', 'done')]}"/>
                <button name="get_binary_pdf_dte" type="object" string="Get PDF" attrs="{'invisible': [('state', '!=', 'done')]}"/>
            </xpath>
        </field>
    </record>

</odoo>
